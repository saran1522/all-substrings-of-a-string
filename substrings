#include<bits/stdc++.h>
using namespace std;
// *************printing all substrings of a string**************
int main(){
    string str, str1;
    cout<<"enter string"<<endl;
    cin>>str;
    for (int i = 0; i < str.length(); i++)
    {
        for (int j = i; j <str.length(); j++)
        {
            for(int k=i; k<=j; k++)
            {
             cout<<str[k];
            }
            cout<<endl;
        }
        
    }
    return 0;
}